{
  "name": "learn-scilla",
  "version": "0.1.0",
  "keywords": [
    "scilla",
    "smart contract",
    "zilliqa",
    "blockchain"
  ],
  "repository": "https://github.com/noelyoo/learn-scilla",
  "author": "Noel Yoo <innovation.noel@gmail.com>",
  "license": "GPL-3.0",
  "jest": {
    "collectCoverageFrom": [
      "src/components/**/*.{ts,tsx}",
      "!src/components/**/*.stories.tsx"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:cov": "react-scripts test --coverage",
    "test:ci": "CI=true react-scripts test --coverage --updateSnapshot --forceExit --detectOpenHandles",
    "eject": "react-scripts eject",
    "tslint": "tslint --project tsconfig.json --format stylish",
    "stylelint": "stylelint ./src/**/*.css --fix",
    "format": "prettier --write '**/*.{ts,tsx,js}' --config .prettierrc",
    "ci": "yarn tslint && yarn stylelint && yarn format && yarn test:ci",
    "deploy": "firebase deploy",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "deploy-storybook": "storybook-to-ghpages",
    "codecov": "codecov -f coverage/*.json"
  },
  "dependencies": {
    "firebase": "^5.5.9",
    "i18next": "^12.1.0",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-helmet": "^5.2.0",
    "react-i18next": "^8.3.8",
    "react-icons": "^3.4.0",
    "react-markdown": "^4.0.3",
    "react-monaco-editor": "^0.22.0",
    "react-redux": "^5.1.1",
    "react-redux-firebase": "^2.2.3",
    "react-router": "^4.4.0-beta.6",
    "react-router-dom": "^4.4.0-beta.6",
    "react-scripts": "2.1.1",
    "reactstrap": "^7.1.0",
    "redux": "^4.0.0",
    "redux-persist": "^5.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@storybook/addon-actions": "^4.1.0-alpha.11",
    "@storybook/addon-links": "^4.1.0-alpha.11",
    "@storybook/addon-options": "^4.1.3",
    "@storybook/addons": "^4.1.0-alpha.11",
    "@storybook/react": "^4.1.0-alpha.11",
    "@storybook/storybook-deployer": "^2.8.1",
    "@types/history": "^4.7.2",
    "@types/jest": "^23.3.10",
    "@types/node": "^11.9.0",
    "@types/react": "^16.8.2",
    "@types/react-dom": "^16.8.0",
    "@types/react-redux": "^6.0.10",
    "@types/react-router": "^4.4.1",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-test-renderer": "^16.8.0",
    "@types/reactstrap": "^6.4.2",
    "@types/storybook__react": "^4.0.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.4",
    "bootstrap": "^4.3.1",
    "codecov": "^3.1.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "jest": "^23.6.0",
    "pre-push": "^0.1.1",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "react-storybook-addon-chapters": "^3.0.3",
    "react-test-renderer": "^16.6.3",
    "redux-logger": "^3.0.6",
    "stylelint": "^9.9.0",
    "stylelint-config-standard": "^18.2.0",
    "ts-jest": "^23.10.5",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.16.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.3"
  },
  "pre-push": {
    "run": [
      "tslint",
      "stylelint",
      "format",
      "test:cov"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
